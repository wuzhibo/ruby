<h1>New order_list</h1>

<% form_for(@order_list) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :order_no %><br />
    <%= f.text_field :order_no %>
  </p>
  <p>
    <%= f.label :order_date %><br />
    <%= f.date_select :order_date %>
  </p>
  <p>
    <%= f.label :state %><br />
    <%= f.text_field :state %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', order_lists_path %>