<h1>Listing order_lists</h1>

<table>
  <tr>
    <th>Order no</th>
    <th>Order date</th>
    <th>State</th>
    <th>User</th>
  </tr>

<% @order_lists.each do |order_list| %>
  <tr>
    <td><%=h order_list.order_no %></td>
    <td><%=h order_list.order_date %></td>
    <td><%=h order_list.state %></td>
    <td><%=h order_list.user_id %></td>
    <td><%= link_to 'Show', order_list %></td>
    <td><%= link_to 'Edit', edit_order_list_path(order_list) %></td>
    <td><%= link_to 'Destroy', order_list, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New order_list', new_order_list_path %>